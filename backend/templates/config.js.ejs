window.appLocale = (localStorage.getItem('LOCALE') || (navigator.languages ? navigator.languages[0] : navigator.language) || window.LOCALE || 'pl').split('-')[0];
window.appLocale = appLocale === 'en' ? 'en' : 'pl';
window.require = {
  baseUrl: '/',
  packages: <%- packages %>,
  paths: <%- paths %>,
  shim: <%- shim %>,
  waitSeconds: 20,
  config: {
    i18n: {
      locale: appLocale
    }
  }
};
