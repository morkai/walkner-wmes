<div class="production-vkb hidden <%- mode.alpha ? 'is-alpha' : '' %> <%- mode.numeric ? 'is-numeric' : '' %> <%- mode.multiline ? 'is-multiline' : '' %>">
  <% if (mode.alpha) { %>
  <% if (mode.numeric) { %>
  <div class="production-vkb-row">
    <button class="btn btn-default" type="button" tabindex="-1" data-key="1">1</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="2">2</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="3">3</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="4">4</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="5">5</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="6">6</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="7">7</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="8">8</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="9">9</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="0">0</button>
  </div>
  <% } %>
  <div class="production-vkb-row">
    <button class="btn btn-default" type="button" tabindex="-1" data-key="Q">Q</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="W">W</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="E">E</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="R">R</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="T">T</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="Y">Y</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="U">U</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="I">I</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="O">O</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="P">P</button>
  </div>
  <div class="production-vkb-row">
    <button class="btn btn-default" type="button" tabindex="-1" data-key="A">A</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="S">S</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="D">D</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="F">F</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="G">G</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="H">H</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="J">J</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="K">K</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="L">L</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="BACKSPACE"><i class="fa fa-long-arrow-left"></i></button>
  </div>
  <div class="production-vkb-row">
    <button class="btn btn-default" type="button" tabindex="-1" data-key="Z">Z</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="X">X</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="C">C</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="V">V</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="B">B</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="N">N</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="M">M</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="LEFT">&lt;</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="RIGHT">&gt;</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="CLEAR"><i class="fa fa-eraser"></i></button>
  </div>
  <% if (mode.multiline) { %>
  <div class="production-vkb-row">
    <button class="btn btn-default" type="button" tabindex="-1" data-key=",">,</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key=".">.</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="?">?</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="(">(</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key=")">)</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key=":">:</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="&quot;">&quot;</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="SPACE">____</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="ENTER"><i class="fa fa-level-down fa-rotate-90"></i></button>
  </div>
  <% } %>
  <% } else if (mode.numeric) { %>
  <div class="production-vkb-row">
    <button class="btn btn-default" type="button" tabindex="-1" data-key="1">1</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="2">2</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="3">3</button>
  </div>
  <div class="production-vkb-row">
    <button class="btn btn-default" type="button" tabindex="-1" data-key="4">4</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="5">5</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="6">6</button>
  </div>
  <div class="production-vkb-row">
    <button class="btn btn-default" type="button" tabindex="-1" data-key="7">7</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="8">8</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="9">9</button>
  </div>
  <div class="production-vkb-row">
    <button class="btn btn-default" type="button" tabindex="-1" data-key="LEFT">&lt;</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="0">0</button>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="RIGHT">&gt;</button>
  </div>
  <div class="production-vkb-row">
    <button class="btn btn-default" type="button" tabindex="-1" data-key="BACKSPACE"><i class="fa fa-long-arrow-left"></i></button>
    <% if (mode.negative) { %>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="-">-</button>
    <% } else { %>
    <button class="btn btn-default" type="button" tabindex="-1" disabled>&nbsp;</button>
    <% } %>
    <button class="btn btn-default" type="button" tabindex="-1" data-key="CLEAR"><i class="fa fa-eraser"></i></button>
  </div>
  <% } %>
</div>
