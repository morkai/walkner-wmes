<form class="well filter-form">
  <%- forms.dateTimeRange({
    idPrefix: idPrefix,
    type: 'date+time',
    startHour: 6,
    property: 'time',
    labels: [{text: t('PROPERTY:time'), ranges: true}]
  }) %>
  <div class="form-group">
    <label for="<%- id('type') %>"><%- t('PROPERTY:type') %></label>
    <select id="<%- id('type') %>" name="type[]" class="form-control is-expandable" multiple size="1">
      <% TYPES.forEach(function(type) { %>
      <option value="<%- type %>"><%- t('type:' + type) %></option>
      <% }) %>
    </select>
  </div>
  <div class="form-group">
    <label for="<%- id('user') %>"><%- t('PROPERTY:user') %></label>
    <input id="<%- id('user') %>" name="user" type="text" autocomplete="new-password">
  </div>
  <div class="form-group">
    <label for="<%- id('order') %>"><%- t('filter:order') %></label>
    <input id="<%- id('order') %>" name="order" class="form-control" type="text" autocomplete="new-password" style="width: 90px">
  </div>
  <div class="form-group">
    <label for="<%- id('line') %>"><%- t('filter:line') %></label>
    <input id="<%- id('line') %>" name="line" type="text" autocomplete="new-password">
  </div>
  <div class="form-group">
    <label for="<%- id('setDate') %>"><%- t('filter:set') %></label>
    <div style="display: flex">
      <input id="<%- id('setDate') %>" name="setDate" class="form-control" type="date" style="width: 130px; margin-right: 1px">
      <input id="<%- id('setNo') %>" name="setNo" class="form-control no-controls" type="number" placeholder="#" style="width: 40px; text-align: center">
    </div>
  </div>
  <%- renderLimit() %>
  <div class="form-group filter-actions">
    <button type="submit" class="btn btn-default"><i class="fa fa-filter"></i><span><%- t('core', 'filter:submit') %></span></button>
  </div>
</form>
