<form class="well filter-form">
  <div class="form-group">
    <label for="<%- idPrefix %>-title"><%- t('sapLaborTimeFixer', 'PROPERTY:title') %></label>
    <input id="<%- idPrefix %>-title" class="form-control" name="title" type="text" autocomplete="new-password">
  </div>
  <%- renderLimit() %>
  <div class="form-group filter-actions">
    <button type="submit" class="btn btn-default"><i class="fa fa-filter"></i><span><%= t('sapLaborTimeFixer', 'filter:submit') %></span></button>
  </div>
</form>
