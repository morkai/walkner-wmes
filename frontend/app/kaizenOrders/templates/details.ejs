<div class="kaizenOrders-details">
  <div class="panel panel-primary kaizenOrders-details-props">
    <div class="panel-heading"><%= model.subject || t('kaizenOrders', 'PANEL:TITLE:details') %></div>
    <div class="panel-details <%- model.changed.all ? 'is-changed' : '' %>">
      <div class="row">
        <div class="col-md-4">
          <div class="props first">
            <div class="prop">
              <div class="prop-name <%- model.changed.types ? 'is-changed' : '' %>"><%- t('kaizenOrders', 'PROPERTY:types') %></div>
              <div class="prop-value"><%- model.types %></div>
            </div>
            <div class="prop">
              <div class="prop-name <%- model.changed.status ? 'is-changed' : '' %>"><%- t('kaizenOrders', 'PROPERTY:status') %></div>
              <div class="prop-value"><%- model.status %></div>
            </div>
            <div class="prop">
              <div class="prop-name <%- model.changed.section ? 'is-changed' : '' %>"><%- t('kaizenOrders', 'PROPERTY:section') %></div>
              <div class="prop-value"><%- model.section %></div>
            </div>
          </div>
        </div>
        <div class="col-md-4">
          <div class="props">
            <div class="prop">
              <div class="prop-name"><%- t('kaizenOrders', 'PROPERTY:creator') %></div>
              <div class="prop-value"><%- model.creator %></div>
            </div>
            <div class="prop">
              <div class="prop-name"><%- t('kaizenOrders', 'PROPERTY:updater') %></div>
              <div class="prop-value"><%- model.updater %></div>
            </div>
            <div class="prop">
              <div class="prop-name <%- model.changed.confirmer ? 'is-changed' : '' %>"><%- t('kaizenOrders', 'PROPERTY:confirmer') %></div>
              <div class="prop-value"><%- model.confirmer %></div>
            </div>
          </div>
        </div>
        <div class="col-md-4">
          <div class="props">
            <div class="prop">
              <div class="prop-name"><%- t('kaizenOrders', 'PROPERTY:createdAt') %></div>
              <div class="prop-value"><%- model.createdAt %></div>
            </div>
            <div class="prop">
              <div class="prop-name"><%- t('kaizenOrders', 'PROPERTY:updatedAt') %></div>
              <div class="prop-value"><%- model.updatedAt %></div>
            </div>
            <div class="prop">
              <div class="prop-name"><%- t('kaizenOrders', 'PROPERTY:confirmedAt') %></div>
              <div class="prop-value"><%- model.confirmedAt || '-' %></div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="row kaizenOrders-details-types">
    <% if (showNearMissPanel) { %>
    <% include detailsNearMissPanel.ejs %>
    <% } %>
    <% if (showSuggestionPanel) { %>
    <% include detailsSuggestionPanel.ejs %>
    <% } %>
    <% if (showKaizenPanel) { %>
    <% include detailsKaizenPanel.ejs %>
    <% } %>
  </div>
  <% if (model.attachments) { %>
  <% include attachments.ejs %>
  <% } %>
</div>
