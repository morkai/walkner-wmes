<form class="well filter-form qiResults-filter">
  <div class="form-group">
    <label><%- t('qiResults', 'filter:result') %></label>
    <div id="<%- idPrefix %>-result" class="btn-group filter-btn-group qiResults-filter-result" data-toggle="buttons">
      <label class="btn btn-default qiResults-filter-ok"><input type="radio" name="result" value="ok"> <%- t('qiResults', 'filter:result:ok') %></label>
      <label class="btn btn-default qiResults-filter-nok"><input type="radio" name="result" value="nok"> <%- t('qiResults', 'filter:result:nok') %></label>
    </div>
  </div>
  <div class="form-group">
    <label for="<%- idPrefix %>-from"><%- t('core', 'filter:date:from') %></label>
    <input id="<%- idPrefix %>-from" class="form-control" name="from" type="date" placeholder="<%- t('core', 'placeholder:date') %>" min="2016-01-01">
  </div>
  <div class="form-group">
    <label for="<%- idPrefix %>-to"><%- t('core', 'filter:date:to') %></label>
    <input id="<%- idPrefix %>-to" class="form-control" name="to" type="date" placeholder="<%- t('core', 'placeholder:date') %>" min="2016-01-01">
  </div>
  <div class="form-group">
    <label for="<%- idPrefix %>-order"><%- t('qiResults', 'filter:order') %></label>
    <input id="<%- idPrefix %>-order" class="form-control" name="order" type="text" maxlength="12">
  </div>
  <div class="form-group">
    <label for="<%- idPrefix %>-productFamily"><%- t('qiResults', 'filter:productFamily') %></label>
    <input id="<%- idPrefix %>-productFamily" name="productFamily" type="text">
  </div>
  <div class="form-group">
    <label for="<%- idPrefix %>-division"><%- t('qiResults', 'filter:division') %></label>
    <input id="<%- idPrefix %>-division" name="division" type="text">
  </div>
  <div class="form-group">
    <label for="<%- idPrefix %>-kind"><%- t('qiResults', 'filter:kind') %></label>
    <input id="<%- idPrefix %>-kind" name="kind" type="text">
  </div>
  <div class="form-group">
    <label for="<%- idPrefix %>-errorCategory"><%- t('qiResults', 'PROPERTY:errorCategory') %></label>
    <input id="<%- idPrefix %>-errorCategory" name="errorCategory" type="text">
  </div>
  <div class="form-group">
    <label for="<%- idPrefix %>-faultCode"><%- t('qiResults', 'PROPERTY:faultCode') %></label>
    <input id="<%- idPrefix %>-faultCode" name="faultCode" type="text">
  </div>
  <div class="form-group">
    <label for="<%- idPrefix %>-inspector"><%- t('qiResults', 'PROPERTY:inspector') %></label>
    <input id="<%- idPrefix %>-inspector" name="inspector" type="text">
  </div>
  <%- renderLimit() %>
  <div class="form-group filter-actions">
    <button type="submit" class="btn btn-default"><i class="fa fa-filter"></i><span><%= t('qiResults', 'filter:submit') %></span></button>
  </div>
</form>
