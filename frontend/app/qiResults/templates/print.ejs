<div class="qiResults-print">
  <h1 class="qiResults-print-date"><%- time.format(model.inspectedAt, 'LL') %></h1>
  <h1 class="qiResults-print-title"><%- t('qiResults', 'print:title', {rid: model.rid}) %></h1>
  <h2 class="qiResults-print-product"><span class="qiResults-print-label"><%- t('qiResults', 'print:product') %></span> <%= model.productName %></h2>
  <h2 class="qiResults-print-problem"><span class="qiResults-print-label"><%- t('qiResults', 'print:problem') %></span> <%= model.problem %></h2>
  <div class="qiResults-print-images">
    <div class="qiResults-print-ok">
      <% if (model.okFile && /^image/.test(model.okFile.type)) { %>
      <img class="qiResults-print-image" src="/qi/results/<%- model._id %>/attachments/okFile?min=<%- model.okFile._id %>">
      <% } %>
    </div>
    <div class="qiResults-print-nok">
      <% if (model.nokFile && /^image/.test(model.nokFile.type)) { %>
      <img class="qiResults-print-image" src="/qi/results/<%- model._id %>/attachments/nokFile?min=<%- model.nokFile._id %>">
      <% } %>
    </div>
  </div>
</div>
