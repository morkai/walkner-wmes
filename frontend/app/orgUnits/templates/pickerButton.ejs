<div class="orgUnits-picker">
  <label class="control-label"><%- label %></label>
  <div class="btn-group">
    <button id="<%- idPrefix %>-showDialog" class="btn btn-default <%- active ? 'active': '' %>" type="button"><%- button %></button>
    <button id="<%- idPrefix %>-clear" class="btn btn-default" type="button" <%- active ? '' : 'disabled' %>><i class="fa fa-eraser"></i></button>
  </div>
</div>
