<form class="orgUnits-picker-dialog">
  <% orgUnits.forEach(function(orgUnit) { %>
  <div class="form-group">
    <label class="radio-inline"><input type="radio" name="orgUnitType" value="<%- orgUnit.type %>"> <%- orgUnit.label %></label>
    <input id="<%- idPrefix %>-<%- orgUnit.type %>" class="orgUnits-picker-orgUnits" name="<%- orgUnit.type %>" type="text" autocomplete="new-password">
  </div>
  <% }) %>
  <div class="form-actions">
    <button type="submit" class="btn btn-primary"><%- t('orgUnits', 'picker:dialog:submit') %></button>
    <button type="button" class="cancel btn btn-link"><%- t('core', 'ACTION_FORM:BUTTON:cancel') %></button>
    <button id="<%- idPrefix %>-deactivated" type="button" class="btn btn-default orgUnits-picker-deactivated <%- deactivated ? 'active' : '' %>"><%- t('orgUnits', 'picker:dialog:deactivated') %></button>
  </div>
</form>
