<form class="well filter-form">
  <%- forms.dateTimeRange({
    idPrefix: idPrefix,
    property: 'scannedAt',
    type: 'date',
    startHour: 6,
    labels: {text: t('prodSerialNumbers', 'PROPERTY:scannedAt'), ranges: true}
  }) %>
  <div class="form-group">
    <label for="<%- idPrefix %>-_id"><%- t('prodSerialNumbers', 'PROPERTY:_id') %></label>
    <input id="<%- idPrefix %>-_id" class="form-control" name="_id" type="text" autocomplete="new-password">
  </div>
  <div class="form-group">
    <label for="<%- idPrefix %>-orderNo"><%- t('prodSerialNumbers', 'PROPERTY:orderNo') %></label>
    <input id="<%- idPrefix %>-orderNo" class="form-control" name="orderNo" type="text" autocomplete="new-password">
  </div>
  <div id="<%- idPrefix %>-orgUnit" class="form-group"></div>
  <%- renderLimit() %>
  <div class="form-group filter-actions">
    <button type="submit" class="btn btn-default"><i class="fa fa-filter"></i><span><%= t('prodSerialNumbers', 'filter:submit') %></span></button>
  </div>
</form>
