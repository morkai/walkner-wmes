<table class="table table-bordered table-condensed pressWorksheets-form-orders">
  <thead>
    <tr>
      <th rowspan="<%- rowspan %>"><%- t('pressWorksheets', 'PROPERTY:order.part') %></th>
      <th rowspan="<%- rowspan %>"><%- t('pressWorksheets', 'PROPERTY:order.operation') %></th>
      <th class="pressWorksheets-form-min" rowspan="<%- rowspan %>"><%- t('pressWorksheets', 'PROPERTY:order.prodLine') %></th>
      <th class="pressWorksheets-form-min" rowspan="<%- rowspan %>"><%- t('pressWorksheets', 'PROPERTY:order.quantityDone') %></th>
      <th class="pressWorksheets-form-min pressWorksheets-form-timeCell" rowspan="<%- rowspan %>"><%- t('pressWorksheets', 'PROPERTY:order.startedAt') %></th>
      <th class="pressWorksheets-form-min pressWorksheets-form-timeCell" rowspan="<%- rowspan %>"><%- t('pressWorksheets', 'PROPERTY:order.finishedAt') %></th>
      <% if (lossReasons.length) { %>
      <th class="pressWorksheets-form-min pressWorksheets-form-separator" colspan="<%- lossReasons.length %>"><%- t('pressWorksheets', 'PROPERTY:order.losses') %></th>
      <% } %>
      <% if (downtimeReasons.length) { %>
      <th class="pressWorksheets-form-min pressWorksheets-form-separator" colspan="<%- downtimeReasons.length %>"><%- t('pressWorksheets', 'PROPERTY:order.downtimes') %></th>
      <% } %>
    </tr>
    <% if (rowspan) { %>
    <tr class="pressWorksheets-form-losses">
      <% lossReasons.forEach(function(lossReason, i) { %>
      <th class="<%- i ? '' : 'pressWorksheets-form-separator' %>"><span class="pressWorksheets-form-loss"><%- lossReason.label %></span></th>
      <% }) %>
      <% downtimeReasons.forEach(function(downtimeReason, i) { %>
      <th class="<%- i ? '' : 'pressWorksheets-form-separator' %>"><span class="pressWorksheets-form-loss"><%- downtimeReason.label %></span></th>
      <% }) %>
    </tr>
    <% } %>
  </thead>
  <tbody></tbody>
</table>
