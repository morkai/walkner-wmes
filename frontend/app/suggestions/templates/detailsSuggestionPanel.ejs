<div class="col-md-<%- suggestionColumnSize %>">
  <div class="panel panel-warning">
    <div class="panel-heading"><%- t('suggestions', 'type:suggestion') %></div>
    <div class="panel-details <%- model.changed.all ? 'is-changed' : '' %>">
      <div class="props first">
        <div class="prop">
          <div class="prop-name <%- model.changed.suggestionOwners ? 'is-changed' : '' %>"><%- t('suggestions', 'PROPERTY:suggestionOwners') %></div>
          <div class="prop-value">
            <ul>
              <% model.suggestionOwners.forEach(function(owner) { %>
              <li><%- owner %></li>
              <% }) %>
            </ul>
          </div>
        </div>
        <div class="prop">
          <div class="prop-name <%- model.changed.date ? 'is-changed' : '' %>"><%- t('suggestions', 'PROPERTY:date') %></div>
          <div class="prop-value"><%- model.date || '-' %></div>
        </div>
        <div class="prop" data-dictionary="categories" data-property="category">
          <div class="prop-name <%- model.changed.categories ? 'is-changed' : '' %>"><%- t('suggestions', 'PROPERTY:categories') %> <i class="fa fa-info-circle"></i></div>
          <div class="prop-value"><%= model.categories %></div>
        </div>
        <div class="prop">
          <div class="prop-name <%- model.changed.productFamily ? 'is-changed' : '' %>"><%- t('suggestions', 'PROPERTY:productFamily') %></div>
          <div class="prop-value"><%- model.productFamily || '-' %></div>
        </div>
        <div class="prop">
          <div class="prop-name <%- model.changed.howItIs ? 'is-changed' : '' %>"><%- t('suggestions', 'PROPERTY:howItIs') %></div>
          <div class="prop-value text-lines"><%- model.howItIs || '-' %></div>
        </div>
        <div class="prop">
          <div class="prop-name <%- model.changed.howItShouldBe ? 'is-changed' : '' %>"><%- t('suggestions', 'PROPERTY:howItShouldBe') %></div>
          <div class="prop-value text-lines"><%- model.howItShouldBe || '-' %></div>
        </div>
        <div class="prop">
          <div class="prop-name <%- model.changed.suggestion ? 'is-changed' : '' %>"><%- t('suggestions', 'PROPERTY:suggestion') %></div>
          <div class="prop-value text-lines"><%- model.suggestion || '-' %></div>
        </div>
      </div>
    </div>
  </div>
</div>
