<div class="panel panel-primary">
  <div class="panel-heading">
    <%- t('subdivisions', 'PANEL:TITLE:details') %>
  </div>
  <div class="panel-details">
    <div class="props first">
      <% ['division', 'type', 'name', 'prodTaskTags', 'aor', 'initialDowntime', 'autoDowntime'].forEach(function(prop) { %>
      <div class="prop">
        <div class="prop-name"><%- t('subdivisions', 'PROPERTY:' + prop) %></div>
        <div class="prop-value"><%- model[prop] || '-' %></div>
      </div>
      <% }) %>
    </div>
  </div>
</div>
