<div class="panel panel-primary">
  <div class="panel-heading"><%- t('companies', 'PANEL:TITLE:details') %></div>
  <div class="panel-details">
    <div class="props first">
      <div class="prop">
        <div class="prop-name"><%- t('companies', 'PROPERTY:_id') %></div>
        <div class="prop-value"><%= model._id %></div>
      </div>
      <div class="prop">
        <div class="prop-name"><%- t('companies', 'PROPERTY:name') %></div>
        <div class="prop-value"><%= model.name || '-' %></div>
      </div>
      <div class="prop">
        <div class="prop-name"><%- t('companies', 'PROPERTY:fteMasterMaster') %></div>
        <div class="prop-value"><%- t('core', 'BOOL:' + model.fteMasterMaster) %></div>
      </div>
      <div class="prop">
        <div class="prop-name"><%- t('companies', 'PROPERTY:fteMaster') %></div>
        <div class="prop-value"><%- t('core', 'BOOL:' + model.fteMaster) %></div>
      </div>
      <div class="prop">
        <div class="prop-name"><%- t('companies', 'PROPERTY:fteLeader') %></div>
        <div class="prop-value"><%- t('core', 'BOOL:' + model.fteMaster) %></div>
      </div>
    </div>
  </div>
</div>
