<div class="panel panel-primary">
  <div class="panel-heading"><%- t('companies', 'PANEL:TITLE:details') %></div>
  <div class="panel-details">
    <div class="props first">
      <div class="prop">
        <div class="prop-name"><%- t('companies', 'PROPERTY:_id') %></div>
        <div class="prop-value"><%= model._id %></div>
      </div>
      <div class="prop">
        <div class="prop-name"><%- t('companies', 'PROPERTY:name') %></div>
        <div class="prop-value"><%= model.name || '-' %></div>
      </div>
      <div class="prop">
        <div class="prop-name"><%- t('companies', 'PROPERTY:fteMasterPosition') %></div>
        <div class="prop-value"><%- model.fteMasterPosition %></div>
      </div>
      <div class="prop">
        <div class="prop-name"><%- t('companies', 'PROPERTY:fteLeaderPosition') %></div>
        <div class="prop-value"><%- model.fteLeaderPosition %></div>
      </div>
      <div class="prop">
        <div class="prop-name"><%- t('companies', 'PROPERTY:color') %></div>
        <div class="prop-value"><%- model.color %></div>
      </div>
    </div>
  </div>
</div>
