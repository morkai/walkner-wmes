<form class="well filter-form" autocomplete="off">
  <div class="form-group">
    <label for="<%- idPrefix %>-name"><%- helpers.t('PROPERTY:name') %></label>
    <input id="<%- idPrefix %>-name" class="form-control" name="name" type="text">
  </div>
  <div class="form-group">
    <label for="<%- idPrefix %>-family"><%- helpers.t('PROPERTY:family') %></label>
    <input id="<%- idPrefix %>-family" class="form-control" name="family" type="text">
  </div>
  <%- renderLimit() %>
  <div class="form-group filter-actions">
    <button type="submit" class="btn btn-default"><i class="fa fa-filter"></i><span><%= helpers.t('filter:submit') %></span></button>
  </div>
</form>
