<table class="paintShop-orderDetails">
  <thead>
  <tr>
    <th><%- t('PROPERTY:no') %></th>
    <th><%- t('PROPERTY:order') %></th>
    <th><%- t('PROPERTY:nc12') %></th>
    <th colspan="2"><%- t('PROPERTY:qty') %></th>
    <th><%- t('PROPERTY:name') %></th>
    <th colspan="3"><%- t('PROPERTY:placement') %></th>
  </tr>
  </thead>
  <% if (canAct) { %>
  <tfoot>
  <tr>
    <td id="<%- id('actions') %>" colspan="9">
      <%- renderOrderActions({order: order}) %>
    </td>
  </tr>
  </tfoot>
  <% } %>
  <%- renderQueueOrder({
    order: order,
    visible: true,
    first: false,
    last: false,
    commentVisible: false,
    canAct: canAct,
    rowSpan: 'rowSpanDetails',
    mrpDropped: mrpDropped,
    getChildOrderDropZoneClass: getChildOrderDropZoneClass,
    details: true
  }) %>
  <tbody>
  <tr>
    <td id="<%- id('whOrders') %>" colspan="9">
      <%- renderWhOrders({order: order}) %>
    </td>
  </tr>
  </tbody>
  <% if (canAct) { %>
  <tbody>
  <tr>
    <td id="<%- id('filler') %>" class="paintShop-orderDetails-filler" colspan="9" style="height: <%- fillerHeight %>px"></td>
  </tr>
  </tbody>
  <% } %>
</table>
