<form id="<%- idPrefix %>-vendorPrintDialog" autocomplete="off">
  <div class="row">
    <div class="col-md-4 form-group">
      <label for="<%- idPrefix %>-printer" class="control-label"><%- t('purchaseOrders', 'PROPERTY:print.printer') %></label>
      <select id="<%- idPrefix %>-printer" class="form-control" name="printer" required autofocus>
        <% printers.forEach(function(printerName) { %>
        <option value="<%= printerName %>"><%= printerName %></option>
        <% }) %>
      </select>
    </div>
    <div class="col-md-8 form-group">
      <label for="<%- idPrefix %>-labelType" class="control-label"><%- t('purchaseOrders', 'vendorPrintDialog:labelType') %></label>
      <select id="<%- idPrefix %>-labelType" class="form-control" name="labelType">
        <% labelTypes.forEach(function(labelType) { %>
          <option value="<%- labelType.id %>"><%= labelType.text %></option>
        <% }) %>
      </select>
    </div>
  </div>
  <div class="form-group pos-printDialog-items-group">
    <div class="table-responsive">
      <table class="table pos-printDialog-items">
        <thead>
          <tr>
            <th><%- t('purchaseOrders', 'PROPERTY:item._id') %></th>
            <th><%- t('purchaseOrders', 'PROPERTY:item.nc12') %></th>
            <th><%- t('purchaseOrders', 'vendorPrintDialog:labelCount') %></th>
            <th><%- t('vendorNc12s', 'PROPERTY:value') %></th>
            <th><%- t('vendorNc12s', 'PROPERTY:unit') %></th>
          </tr>
        </thead>
        <tbody>
          <% items.forEach(function(item, i) { %>
          <tr class="pos-printDialog-items-item">
            <td>
              <%- item._id %>
              <input name="items[<%- i %>]._id" type="hidden" value="<%- item._id %>">
            </td>
            <td>
              <%- item.nc12 %>
              <input name="items[<%- i %>].nc12" type="hidden" value="<%- item.nc12 %>">
            </td>
            <td><input class="form-control no-controls pos-printDialog-qty" name="items[<%- i %>].labelCount" type="text" max="9999999"></td>
            <td><input class="form-control" name="items[<%- i %>].value" type="text" ></td>
            <td><input class="form-control" name="items[<%- i %>].unit" type="text"></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
  </div>
  <div class="form-actions">
    <button type="submit" class="btn btn-primary"><i class="fa fa-print"></i><span><%- t('purchaseOrders', 'printDialog:print') %></span></button>
    <button type="button" class="cancel btn btn-link"><%- t('core', 'ACTION_FORM:BUTTON:cancel') %></button>
  </div>
</form>
