<form id="<%- idPrefix %>-printDialog" autocomplete="off" action="<%= action %>">
  <div class="row">
    <div class="col-md-4 form-group">
      <label for="<%- idPrefix %>-shippingNo"><%- t('purchaseOrders', 'PROPERTY:item.prints.shippingNo') %></label>
      <input id="<%- idPrefix %>-shippingNo" name="shippingNo" class="form-control pos-printDialog-input" type="text" maxlength="20" autofocus>
    </div>
    <div class="col-md-4 form-group">
      <label for="<%- idPrefix %>-paper" class="control-label"><%- t('purchaseOrders', 'PROPERTY:item.prints.paper') %></label>
      <select id="<%- idPrefix %>-paper" class="form-control" name="paper">
        <% papers.forEach(function(paper) { %>
        <option value="<%- paper.id %>"><%- paper.text %></option>
        <% }) %>
      </select>
    </div>
    <div class="col-md-4 form-group">
      <label for="<%- idPrefix %>-barcode" class="control-label"><%- t('purchaseOrders', 'PROPERTY:item.prints.barcode') %></label>
      <select id="<%- idPrefix %>-barcode" class="form-control" name="barcode">
        <% barcodes.forEach(function(barcode) { %>
        <option value="<%- barcode.id %>"><%- barcode.text %></option>
        <% }) %>
      </select>
    </div>
  </div>
  <div class="form-group pos-printDialog-items-group">
    <div class="table-responsive">
      <table class="table pos-printDialog-items">
        <thead>
          <tr>
            <th><%- t('purchaseOrders', 'PROPERTY:item._id') %></th>
            <th><%- t('purchaseOrders', 'PROPERTY:item.nc12') %></th>
            <th><%- t('purchaseOrders', 'PROPERTY:item.prints.packageQty') %></th>
            <th><%- t('purchaseOrders', 'PROPERTY:item.prints.componentQty') %></th>
            <th><%- t('purchaseOrders', 'PROPERTY:item.prints.remainingQty') %></th>
            <th><%- t('purchaseOrders', 'PROPERTY:item.prints.totalPackageQty') %></th>
            <th><%- t('purchaseOrders', 'PROPERTY:item.prints.totalQty') %></th>
            <th></th>
          </tr>
        </thead>
        <tfoot>
          <tr>
            <td colspan="2" class="pos-printDialog-items-total"><%- t('purchaseOrders', 'printDialog:total') %></td>
            <td id="<%- idPrefix %>-overallPackageQty">0</td>
            <td id="<%- idPrefix %>-overallComponentQty">0</td>
            <td id="<%- idPrefix %>-overallRemainingQty">0</td>
            <td id="<%- idPrefix %>-overallTotalPackageQty">0</td>
            <td id="<%- idPrefix %>-overallTotalQty">0</td>
            <td></td>
          </tr>
        </tfoot>
        <tbody>
          <% items.forEach(function(item, i) { %>
          <tr class="pos-printDialog-items-item">
            <td>
              <%- item._id %>
              <input name="items[<%- i %>]._id" type="hidden" value="<%- item._id %>">
            </td>
            <td><%- item.nc12 %></td>
            <td><input class="form-control no-controls pos-printDialog-qty" name="items[<%- i %>].packageQty" type="text" max="999" data-integer></td>
            <td><input class="form-control no-controls pos-printDialog-qty" name="items[<%- i %>].componentQty" type="text" max="9999999"></td>
            <td><input class="form-control no-controls pos-printDialog-qty" name="items[<%- i %>].remainingQty" type="text" max="9999999"></td>
            <td class="pos-printDialog-items-totalPackageQty">0</td>
            <td class="pos-printDialog-items-totalQty">0</td>
            <td></td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
  </div>
  <div class="form-actions">
    <button type="submit" class="btn btn-primary"><i class="fa fa-print"></i><span><%- t('purchaseOrders', 'printDialog:print') %></span></button>
    <button type="button" class="cancel btn btn-link"><%- t('core', 'ACTION_FORM:BUTTON:cancel') %></button>
  </div>
</form>
