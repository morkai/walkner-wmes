<div class="orderDocumentTree-files-item orderDocumentTree-files-file <%- file.selected ? 'is-selected' : '' %>" <% if (file.id) { %>data-id="<%- file.id %>"<% } %>>
  <span class="orderDocumentTree-files-icon"><i class="fa <%- file.icon || 'fa-file-o' %>"></i></span>
  <span class="orderDocumentTree-files-label"><span class="orderDocumentTree-files-label-text"><%- file.text %></span><span class="orderDocumentTree-files-label-smallText"><%= file.smallText %></span></span>
  <% if (file.id) { %>
  <span class="orderDocumentTree-files-files">
    <% _.forEach(file.files, function(f) { %>
    <a href="/orderDocuments/<%- file.id %>?pdf=1&hash=<%- f.hash %>" target="_blank"><%- time.format(f.date, 'L') %></a>
    <% }) %>
  </span>
  <% } %>
</div>
