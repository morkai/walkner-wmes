<form class="well filter-form">
  <div class="form-group">
    <label for="<%- idPrefix %>-division"><%- t('hourlyPlans', 'heff:filter:division') %></label>
    <input id="<%- idPrefix %>-division" name="division" type="text" autocomplete="new-password">
  </div>
  <div class="form-group">
    <label for="<%- idPrefix %>-prodFlows"><%- t('hourlyPlans', 'heff:filter:prodFlows') %></label>
    <input id="<%- idPrefix %>-prodFlows" name="prodFlows" type="text" autocomplete="new-password">
  </div>
  <div class="form-group filter-actions">
    <button type="submit" class="btn btn-default"><i class="fa fa-filter"></i><span><%= t('hourlyPlans', 'heff:filter:submit') %></span></button>
  </div>
</form>
