<form class="well filter-form opinionSurveyScanTemplates-filter">
  <div class="form-group">
    <label for="<%- idPrefix %>-survey"><%- helpers.t('PROPERTY:survey') %></label>
    <input id="<%- idPrefix %>-survey" name="survey" type="text" autocomplete="new-password">
  </div>
  <%- renderLimit() %>
  <div class="form-group filter-actions">
    <button type="submit" class="btn btn-default"><i class="fa fa-filter"></i><span><%= helpers.t('filter:submit') %></span></button>
  </div>
</form>
