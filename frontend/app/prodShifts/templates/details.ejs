<div class="panel panel-<%- panelType %>">
  <div class="panel-heading">
    <%- t('prodShifts', 'PANEL:TITLE:details') %>
  </div>
  <div class="panel-details row">
    <div class="col-md-4">
      <div class="props first">
        <% ['prodLine', 'date', 'shift', 'createdAt', 'creator'].forEach(function(prop) { %>
        <div class="prop">
          <div class="prop-name"><%- t('prodShifts', 'PROPERTY:' + prop) %></div>
          <div class="prop-value"><%- model[prop] %></div>
        </div>
        <% }) %>
      </div>
    </div>
    <div class="col-md-4">
      <div class="props">
        <% ['division', 'subdivision', 'mrpControllers', 'prodFlow', 'workCenter'].forEach(function(prop) { %>
        <div class="prop">
          <div class="prop-name"><%- t('core', 'ORG_UNIT:' + prop) %></div>
          <div class="prop-value"><%- model[prop] %></div>
        </div>
        <% }) %>
      </div>
    </div>
    <div class="col-md-4">
      <div class="props">
        <% ['master', 'leader', 'operator', 'totalQuantityDone', 'efficiency'].forEach(function(prop) { %>
        <div class="prop">
          <div class="prop-name"><%- t('prodShifts', 'PROPERTY:' + prop) %></div>
          <div class="prop-value"><%- model[prop] %></div>
        </div>
        <% }) %>
      </div>
    </div>
  </div>
</div>
