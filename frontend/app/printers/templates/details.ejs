<div class="panel panel-primary">
  <div class="panel-heading"><%- t('printers', 'PANEL:TITLE:details') %></div>
  <div class="panel-details">
    <div class="props first">
      <div class="prop">
        <div class="prop-name"><%- t('printers', 'PROPERTY:name') %></div>
        <div class="prop-value"><%= model.name %></div>
      </div>
      <div class="prop">
        <div class="prop-name"><%- t('printers', 'PROPERTY:label') %></div>
        <div class="prop-value"><%= model.label || '-' %></div>
      </div>
    </div>
  </div>
</div>
