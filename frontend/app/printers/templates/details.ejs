<div class="panel panel-primary">
  <div class="panel-heading"><%- t('printers', 'PANEL:TITLE:details') %></div>
  <div class="panel-details">
    <div class="props first">
      <% ['name', 'label', 'tags'].forEach(function(prop) { %>
      <div class="prop">
        <div class="prop-name"><%- t('printers', 'PROPERTY:' + prop) %></div>
        <div class="prop-value"><%= model[prop] %></div>
      </div>
      <% }) %>
      <div class="prop">
        <div class="prop-name"><%- t('printers', 'PROPERTY:special') %></div>
        <div class="prop-value">
          <% if (model.special) { %>
          <code><%= model.special %></code>
          <% } %>
        </div>
      </div>
    </div>
  </div>
</div>
