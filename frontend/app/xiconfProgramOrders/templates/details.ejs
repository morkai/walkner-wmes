<div>
  <div class="panel <%- programPanelClassName %> xiconfProgramOrders-programOrderDetails">
    <div class="panel-heading">
      <%- t('xiconfProgramOrders', 'PANEL:TITLE:details:programOrder') %>
    </div>
    <div class="panel-details row">
      <div class="col-md-4">
        <div class="props first">
          <div class="prop">
            <div class="prop-name"><%- t('xiconf', 'PROPERTY:no') %></div>
            <div class="prop-value">
              <a href="<%- linkToResults(model._id) %>" title="<%- t('xiconfProgramOrders', 'details:showResultsLink') %>"><%- model._id %></a>
            </div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('xiconfProgramOrders', 'PROPERTY:quantityTodo') %></div>
            <div class="prop-value"><%- model.quantityTodo.toLocaleString() %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('xiconfProgramOrders', 'PROPERTY:quantityDone') %></div>
            <div class="prop-value"><%- model.quantityDone.toLocaleString() %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('xiconfProgramOrders', 'PROPERTY:serviceTagCounter') %></div>
            <div class="prop-value"><%- model.serviceTagCounter.toLocaleString() %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('xiconfProgramOrders', 'PROPERTY:createdAt') %></div>
            <div class="prop-value"><%- model.createdAt ? time.format(model.createdAt, 'LLLL') : '-' %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('xiconfProgramOrders', 'PROPERTY:finishedAt') %></div>
            <div class="prop-value"><%- model.finishedAt ? time.format(model.finishedAt, 'LLLL') : '-' %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('xiconfProgramOrders', 'PROPERTY:duration') %></div>
            <div class="prop-value"><%- model.finishedAt ? time.toString((Date.parse(model.finishedAt) - Date.parse(model.startedAt)) / 1000) : '-' %></div>
          </div>
        </div>
      </div>
      <% model.nc12.forEach(function(nc12) { %>
      <div class="col-md-4">
        <div class="props">
          <div class="prop">
            <div class="prop-name"><%- t('xiconf', 'PROPERTY:nc12') %></div>
            <div class="prop-value">
              <a href="<%- linkToResults(model._id, nc12._id) %>" title="<%- t('xiconfProgramOrders', 'details:showResultsLink') %>"><%- nc12._id %></a>
            </div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('xiconfProgramOrders', 'PROPERTY:quantityTodo') %></div>
            <div class="prop-value"><%- nc12.quantityTodo.toLocaleString() %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('xiconfProgramOrders', 'PROPERTY:quantityDone') %></div>
            <div class="prop-value"><%- nc12.quantityDone.toLocaleString() %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('xiconf', 'PROPERTY:programName') %></div>
            <div class="prop-value"><%= nc12.name || '-' %></div>
          </div>
        </div>
      </div>
      <% }) %>
    </div>
  </div>
</div>
