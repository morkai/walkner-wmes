<div class="panel panel-default d8Entries-history">
  <div class="panel-heading"><%- t('d8Entries', 'PANEL:TITLE:changes') %></div>
  <% items.forEach(function(item, itemIndex) { %>
  <% include historyItem.ejs %>
  <% }) %>
  <form class="d8Entries-history-form">
    <div class="form-group">
      <label for="<%- idPrefix %>-comment" class="control-label"><%- t('d8Entries', 'PROPERTY:comment') %></label>
      <textarea id="<%- idPrefix %>-comment" class="form-control" name="comment" rows="3"></textarea>
    </div>
    <div class="panel-footer">
      <button id="<%- idPrefix %>-submit" type="submit" class="btn btn-primary">
        <i class="fa fa-comment-o"></i><span><%- t('d8Entries', 'history:submit') %></span>
      </button>
    </div>
  </form>
</div>
