<form class="well filter-form xiconf-filter">
  <%- forms.dateTimeRange({
    idPrefix: idPrefix,
    property: 'startedAt',
    type: 'date',
    required: true,
    startHour: 6,
    labels: {text: helpers.t('PROPERTY:startedAt'), ranges: true}
  }) %>
  <div class="form-group">
    <label for="<%- idPrefix %>-prodLine"><%- helpers.t('PROPERTY:prodLine') %></label>
    <input id="<%- idPrefix %>-prodLine" name="prodLine" type="text">
  </div>
  <div class="form-group">
    <label for="<%- idPrefix %>-search"><%- helpers.t('filter:search') %></label>
    <input id="<%- idPrefix %>-search" class="form-control" name="search" type="text">
  </div>
  <div class="form-group">
    <label><%- helpers.t('PROPERTY:result') %></label>
    <div id="<%- idPrefix %>-result" class="btn-group filter-btn-group" data-toggle="buttons">
      <label class="btn btn-default" style="color: #00af00; font-weight: bold; width: 75px">
        <input type="checkbox" name="result[]" value="success">
        <%- helpers.t('filter:result:success') %>
      </label>
      <label class="btn btn-default" style="color: #E00; font-weight: bold; width: 75px">
        <input type="checkbox" name="result[]" value="failure">
        <%- helpers.t('filter:result:failure') %>
      </label>
    </div>
  </div>
  <%- renderLimit() %>
  <div class="form-group filter-actions">
    <button type="submit" class="btn btn-default"><i class="fa fa-filter"></i><span><%= helpers.t('core', 'filter:submit') %></span></button>
  </div>
</form>
