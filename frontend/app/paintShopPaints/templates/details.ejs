<div class="panel panel-primary">
  <div class="panel-heading"><%- t('paintShopPaints', 'PANEL:TITLE:details') %></div>
  <div class="panel-details">
    <div class="props first">
      <% ['_id', 'shelf', 'bin', 'name'].forEach(function(prop) { %>
      <div class="prop">
        <div class="prop-name"><%- t('paintShopPaints', 'PROPERTY:' + prop) %></div>
        <div class="prop-value"><%= model[prop] %></div>
      </div>
      <% }) %>
    </div>
  </div>
</div>
