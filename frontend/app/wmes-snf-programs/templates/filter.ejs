<form class="well filter-form">
  <div class="form-group">
    <label for="<%- idPrefix %>-name"><%- helpers.t('PROPERTY:name') %></label>
    <input id="<%- idPrefix %>-name" class="form-control" name="name" type="text">
  </div>
  <%- renderLimit() %>
  <div class="form-group filter-actions">
    <button type="submit" class="btn btn-default"><i class="fa fa-filter"></i><span><%- helpers.t('core', 'filter:submit') %></span></button>
  </div>
</form>
