<dl>
  <dt><%- t('wh', 'list:popover:status') %></dt>
  <dd><%- status %></dd>
  <% if (user) { %>
  <dt><%- t('wh', 'list:popover:user') %></dt>
  <dd><%- user %></dd>
  <% } %>
  <% if (carts.length) { %>
  <dt><%- t('wh', 'prop:carts') %></dt>
  <dd><%- carts.join(', ') %></dd>
  <% } %>
  <% if (problemArea) { %>
  <dt><%- t('wh', 'prop:problemArea') %></dt>
  <dd><%= problemArea %></dd>
  <% } %>
  <% if (comment) { %>
  <dt><%- t('wh', 'prop:comment') %></dt>
  <dd style="white-space: pre-line"><%= comment %></dd>
  <% } %>
</dl>
