<div class="wh-problems-item" data-id="<%- item._id %>">
  <div class="wh-problems-column">
    <div class="wh-problems-prop">
      <div class="wh-problems-prop-name"><%- t('prop:order') %></div>
      <div class="wh-problems-prop-value">
        <% if (item.urls.order) { %>
        <a href="<%- item.urls.order %>"><%- item.order %></a>
        <% } else { %>
        <%- item.order %>
        <% } %>
      </div>
    </div>
    <div class="wh-problems-prop">
      <div class="wh-problems-prop-name"><%- t('prop:line') %></div>
      <div class="wh-problems-prop-value"><%= item.line %></div>
    </div>
    <div class="wh-problems-prop">
      <div class="wh-problems-prop-name"><%- t('prop:qty') %></div>
      <div class="wh-problems-prop-value"><%= item.qty %></div>
    </div>
  </div>
  <div class="wh-problems-column">
    <div class="wh-problems-prop">
      <div class="wh-problems-prop-name"><%- t('prop:date') %></div>
      <div class="wh-problems-prop-value">
        <% if (item.urls.pickup) { %>
        <a href="<%- item.urls.pickup %>"><%- item.date %></a>
        <% } else { %>
        <%- item.date %>
        <% } %>
      </div>
    </div>
    <div class="wh-problems-prop">
      <div class="wh-problems-prop-name"><%- t('prop:orderTime') %></div>
      <div class="wh-problems-prop-value"><%- item.startTime %></div>
    </div>
    <div class="wh-problems-prop">
      <div class="wh-problems-prop-name"><%- t('prop:problemAt') %></div>
      <div class="wh-problems-prop-value"><%- item.problemAt %></div>
    </div>
  </div>
  <% Object.keys(item.funcs).forEach(function(func) { %>
  <div class="wh-problems-column wh-problems-func <%- item.funcs[func].problem ? '' : 'wh-problems-func-noProblem' %> <%- item.funcs[func].className %>">
    <div class="wh-problems-prop">
      <div class="wh-problems-prop-name"><%- item.funcs[func].label %></div>
      <div class="wh-problems-prop-value"><%- item.funcs[func].status %></div>
    </div>
    <% if (item.funcs[func].user) { %>
    <div class="wh-problems-prop">
      <div class="wh-problems-prop-name"><%- t('prop:user') %></div>
      <div class="wh-problems-prop-value"><%- item.funcs[func].user %></div>
    </div>
    <% } %>
    <% if (item.funcs[func].carts) { %>
    <div class="wh-problems-prop">
      <div class="wh-problems-prop-name"><%- t('prop:carts') %></div>
      <div class="wh-problems-prop-value"><%- item.funcs[func].carts %></div>
    </div>
    <% } %>
    <% if (item.funcs[func].problemArea) { %>
    <div class="wh-problems-prop">
      <div class="wh-problems-prop-name"><%- t('prop:problemArea') %></div>
      <div class="wh-problems-prop-value"><%- item.funcs[func].problemArea %></div>
    </div>
    <% } %>
  </div>
  <% if (item.funcs[func].problem) { %>
  <div class="wh-problems-column wh-problems-problem <%- item.funcs[func].className %>">
    <div class="wh-problems-prop">
      <div class="wh-problems-prop-name"><%- t('prop:problem') %></div>
      <div class="wh-problems-prop-value"><%- item.funcs[func].problem %></div>
    </div>
  </div>
  <% } %>
  <% }) %>
</div>
