<form class="well filter-form osh-report-filter" autocomplete="off">
  <div class="form-group">
    <label for="<%- id('month') %>" class="control-label"><%- t('filter:month') %></label>
    <input id="<%- id('month') %>" name="month" type="month" class="form-control" required placeholder="YYYY-MM">
  </div>
  <div id="<%- id('orgUnit') %>" class="form-group"></div>
  <div class="form-group filter-actions">
    <%- renderButton() %>
  </div>
</form>
