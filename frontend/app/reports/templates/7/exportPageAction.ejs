<div class="btn-group">
  <button type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown">
    <i class="fa fa-download"></i><span><%- t('reports', 'PAGE_ACTION:7:export') %></span> <span class="caret"></span>
  </button>
  <ul class="dropdown-menu">
    <% Object.keys(urls).forEach(function(type) { %>
    <li><a href="<%- urls[type] %>" data-export="<%- type %>"><%- t('reports', 'PAGE_ACTION:7:export:' + type) %></a></li>
    <% }) %>
  </ul>
</div>
