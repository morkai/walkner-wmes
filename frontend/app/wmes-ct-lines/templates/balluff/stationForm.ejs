<div class="panel panel-default">
  <div class="panel-heading is-with-actions">
    <span class="panel-heading-title"></span>
    <% include ../_stationPanelActions.ejs %>
  </div>
  <div class="panel-body">
    <table>
      <tbody>
      <tr>
        <td>
          <label for="<%- id('processorIp') %>" class="control-label"><%- t('PROPERTY:stations:processorIp') %></label>
          <input id="<%- id('processorIp') %>" class="form-control" type="text" name="stations[<%- stationIndex %>].processorIp">
        </td>
        <td>
          <label for="<%- id('headPort') %>" class="control-label"><%- t('PROPERTY:stations:headPort') %></label>
          <input id="<%- id('headPort') %>" class="form-control" type="number" name="stations[<%- stationIndex %>].headPort" min="1" max="65535">
        </td>
        <td>
          <label for="<%- id('headNo') %>" class="control-label"><%- t('PROPERTY:stations:headNo') %></label>
          <input id="<%- id('headNo') %>" class="form-control" type="number" name="stations[<%- stationIndex %>].headNo" min="1" max="8">
        </td>
        <td>
          <label for="<%- id('beamPower') %>" class="control-label"><%- t('PROPERTY:stations:beamPower') %></label>
          <input id="<%- id('beamPower') %>" class="form-control" type="number" name="stations[<%- stationIndex %>].beamPower" min="7" max="27">
        </td>
      </tr>
      <tr>
        <td>
          <label for="<%- id('redLampI') %>" class="control-label"><%- t('PROPERTY:stations:redLampI') %></label>
          <input id="<%- id('redLampI') %>" class="form-control" type="number" name="stations[<%- stationIndex %>].redLampI">
        </td>
        <td>
          <label for="<%- id('orangeLampI') %>" class="control-label"><%- t('PROPERTY:stations:orangeLampI') %></label>
          <input id="<%- id('orangeLampI') %>" class="form-control" type="number" name="stations[<%- stationIndex %>].orangeLampI">
        </td>
        <td></td>
        <td></td>
      </tr>
      </tbody>
    </table>
  </div>
</div>
