<form class="well filter-form" autocomplete="off">
  <%- helpers.formGroup({
    name: 'status',
    type: 'select',
    label: 'PROPERTY:',
    expandable: true,
    options: statuses,
    groupAttrs: {'data-filter': 'status'}
  }) %>
  <%- forms.dateTimeRange({
    idPrefix,
    hidden: true,
    labels: ['createdAt', 'startedAt', 'implementedAt', 'plannedAt', 'finishedAt'].map(value => ({
      text: t(`PROPERTY:${value}`),
      ranges: true,
      value
    }))
  }) %>
  <div class="form-group" style="min-width: 280px">
    <input id="<%- id('userType') %>" name="userType" type="hidden" value="others">
    <input id="<%- id('user') %>" name="user" type="text">
  </div>
  <div id="<%- id('locationPath') %>" class="form-group hidden" data-filter="locationPath"></div>
  <%- helpers.formGroup({
    name: 'kind',
    type: 'select',
    label: 'PROPERTY:',
    groupClassName: 'hidden',
    groupAttrs: {'data-filter': 'kind'},
    expandable: true,
    options: kinds
  }) %>
  <%- helpers.formGroup({
    name: 'activityKind',
    type: 'select2',
    label: 'PROPERTY:',
    groupClassName: 'hidden',
    groupAttrs: {'data-filter': 'activityKind'}
  }) %>
  <%- renderLimit({hidden: true}) %>
  <div class="form-group filter-actions">
    <%- renderButton() %>
  </div>
</form>
