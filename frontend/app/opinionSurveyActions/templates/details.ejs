  <div class="row">
    <div class="col-lg-3">
      <div class="panel panel-default">
        <div class="panel-heading"><%- t('opinionSurveyActions', 'PROPERTY:question') %></div>
        <div class="panel-body">
          <p class="opinionSurveyActions-details-text"><%= model.question %></p>
        </div>
      </div>
    </div>
    <div class="col-lg-3">
      <div class="panel panel-danger">
        <div class="panel-heading"><%- t('opinionSurveyActions', 'PROPERTY:problem') %></div>
        <div class="panel-body">
          <p class="opinionSurveyActions-details-text"><%= model.problem %></p>
