            <div class="prop-value"><%- model.createdAt %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('opinionSurveyActions', 'PROPERTY:creator') %></div>
            <div class="prop-value"><%- model.creator %></div>
