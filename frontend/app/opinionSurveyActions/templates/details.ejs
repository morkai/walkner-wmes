<div>
  <div class="panel panel-primary">
    <div class="panel-heading"><%- t('opinionSurveyActions', 'PANEL:TITLE:details') %></div>
    <div class="panel-details row">
      <div class="col-md-6">
        <div class="props first">
          <div class="prop">
            <div class="prop-name"><%- t('opinionSurveyActions', 'PROPERTY:rid') %></div>
            <div class="prop-value"><%- model.rid %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('opinionSurveyActions', 'PROPERTY:survey') %></div>
            <div class="prop-value"><%= model.survey %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('opinionSurveyActions', 'PROPERTY:status') %></div>
            <div class="prop-value"><%- model.status %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('opinionSurveyActions', 'PROPERTY:division') %></div>
            <div class="prop-value"><%= model.division %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('opinionSurveyActions', 'PROPERTY:superior') %></div>
            <div class="prop-value"><%- model.superior %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('opinionSurveyActions', 'PROPERTY:owners') %></div>
            <div class="prop-value"><%- model.owners %></div>
          </div>
        </div>
      </div>
      <div class="col-md-6">
        <div class="props">
          <div class="prop">
            <div class="prop-name"><%- t('opinionSurveyActions', 'PROPERTY:startDate') %></div>
            <div class="prop-value"><%- model.startDate %></div>
          </div>
          <div class="prop">
            <div class="prop-name"><%- t('opinionSurveyActions', 'PROPERTY:finishDate') %></div>
            <div class="prop-value"><%- model.finishDate %></div>
          </div>
          <div class="prop">
