<div class="panel panel-primary">
  <div class="panel-heading">
    <%- t('prodLines', 'PANEL:TITLE:details') %>
  </div>
  <div class="panel-details">
    <div class="props first">
      <div class="prop">
        <div class="prop-name"><%- t('prodLines', 'PROPERTY:workCenter') %></div>
        <div class="prop-value"><%- orgUnitPath || '-' %></div>
      </div>
      <div class="prop">
        <div class="prop-name"><%- t('prodLines', 'PROPERTY:_id') %></div>
        <div class="prop-value"><%= model._id %></div>
      </div>
      <div class="prop">
        <div class="prop-name"><%- t('prodLines', 'PROPERTY:description') %></div>
        <div class="prop-value"><%= model.description || '-' %></div>
      </div>
    </div>
  </div>
</div>
